    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "nodemon": "^2.0.4",
    "pg": "^8.2.2",
    "react-redux": "^7.2.0",
    "save": "^2.4.0",
    "sequelize": "^6.20.1",
    "sequelize-cli": "^6.2.0"
  }
}
